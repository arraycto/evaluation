<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">

    <diskStore path="java.io.tmpdir/third"/>
    <defaultCache
            maxElementsInMemory="1000"
            timeToIdleSeconds="604800"
            timeToLiveSeconds="604800"
            maxElementsOnDisk="10000"
            diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </defaultCache>
    <cache name="allThirdKpi" maxElementsInMemory="1000"
           timeToIdleSeconds="604800"
           timeToLiveSeconds="604800"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="secondKpiId" maxElementsInMemory="500"
           timeToIdleSeconds="604800"
           timeToLiveSeconds="604800"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="secondKpiIdAndThirdKpiIds" maxElementsInMemory="500"
           timeToIdleSeconds="604800"
           timeToLiveSeconds="604800"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="secondKpiIdsAndThirdKpiIds" maxElementsInMemory="500"
           timeToIdleSeconds="604800"
           timeToLiveSeconds="604800"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="secondKpiIds" maxElementsInMemory="500"
           timeToIdleSeconds="604800"
           timeToLiveSeconds="604800"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="localTempSwap"/>
    </cache>
</ehcache>